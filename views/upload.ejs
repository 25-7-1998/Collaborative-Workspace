<div class="container-fluid">


  <div class="card">

    <div class="card-body">

      <h2 class="card-title">Upload File</h2>
      <div class="col-lg-6">
        <form class="" action="index.html" method="post">
          <div class="input-group mb-3">
            <div class="custom-file">
              <input type="file" class="custom-file-input" id="input">
              <label class="custom-file-label" for="input" aria-describedby="inputGroupFileAddon02">Choose file</label>
            </div>
            <div class="input-group-append">
              <form class="" action="/upload" method="get">
                <button class="input-group-text" id="inputGroupFileAddon02" type="submit">Upload</button>
              </form>
            </div>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>

