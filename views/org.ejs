<%# members %>
<div class="container-fluid">
  <div class="row">
    <div class="col-lg-6">
      <div class="card">
        <div class="card-body">
          <h2 class="card-title">Members</h2>
          <ol class="list-group">
            <%for(var i=0;i<org.members.length;i++){%>
            <tr>
            <div class="row">

            <div class="col-lg-2 col-md-2 memb">
                <td ><%=i+1%></td>
            </div>

            <div class="col-lg-6 col-md-5 memb">
                <td><%=org.members[i]%></td>
            </div>

            <div class="col-lg-2 col-md-2 memb">

            <form class="" action="/list/org.members[i]" method="get">
              <input type="hidden" value="<%=org.name%>" name="org_name" />
              <input type="hidden" value="<%=org.members[i]%>" name="user" />

              <td><button type="submit" name="task" class="btn btn-dark" ><i class="fas fa-thumbtack"></i></button></td>
            </form>


            </div>

            <div class="col-lg-2 col-md-2 memb">
                <td><button type="submit" name="remove" class="btn btn-dark"><i class="far fa-times-circle"></i></button></td>
            </div>


            </div>
            </tr>
            <%}%>
          </ol>
        </div>
      </div>
  </div>

  <div class="col-lg-6">

  </div>

</div>
</div>

<!-- Footer -->

